<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="com.hncgc1990.rxjavademo.operator.filterOperator.FilterMainActivity">



    <EditText
        android:id="@+id/et_search"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="输入关键字"/>



    <Button
        android:id="@+id/btn_debounce"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="debounce操作符"/>



    <Button
        android:id="@+id/btn_throttleWithTimeout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="throttleWithTimeout操作符"/>


    <Button
        android:id="@+id/btn_distinct"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="distinct操作符"/>
    <Button
        android:id="@+id/btn_distinctUtilChange"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="distinctUtilChange操作符"/>


    <Button
        android:id="@+id/btn_elementAt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="elementAt操作符"/>

    <Button
        android:id="@+id/btn_elementAtOrDefault"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="elementAtOrDefault操作符"/>


    <Button
        android:id="@+id/btn_filter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="filter操作符"/>


    <Button
        android:id="@+id/btn_first"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="first操作符"/>



    <Button
        android:id="@+id/btn_single"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="single操作符"/>

    <Button
        android:id="@+id/btn_ignoreElement"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ignoreElement操作符"/>


    <Button
        android:id="@+id/btn_last"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="last操作符"/>

    <Button
        android:id="@+id/btn_sample"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sample操作符"/>



    <Button
        android:id="@+id/btn_skip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="skip操作符"/>


    <Button
        android:id="@+id/btn_skiplast"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="skipLast操作符"/>


    <Button
        android:id="@+id/btn_take"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="take操作符"/>

    <Button
        android:id="@+id/btn_timeout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="timeout操作符"/>


    <Button
        android:id="@+id/btn_throttleFirst"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="throttleFirst操作符"/>

</LinearLayout>
</ScrollView>