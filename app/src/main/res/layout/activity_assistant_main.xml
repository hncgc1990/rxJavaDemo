<?xml version="1.0" encoding="utf-8"?>

<ScrollView  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="com.hncgc1990.rxjavademo.operator.assistantOperator.AssistantMainActivity">



    <Button
        android:id="@+id/btn_delay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="delay操作符"/>

    <Button
        android:id="@+id/btn_delaySubscription"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="delaySubscription操作符"/>


    <Button
        android:id="@+id/btn_dooneach"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="doOnEach操作符"/>

    <Button
        android:id="@+id/btn_doonnext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="doOnNext操作符"/>

    <Button
        android:id="@+id/btn_doonsubscribe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="doOnSubscribe操作符"/>

    <Button
        android:id="@+id/btn_doonerror"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="doOnError操作符"/>

    <Button
        android:id="@+id/btn_dooncomplete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="doOncomplete操作符"/>

    <Button
        android:id="@+id/btn_doonterminate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="doOnterminate操作符"/>


    <Button
        android:id="@+id/btn_materialize"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="materialize操作符"/>

    <Button
        android:id="@+id/btn_dematerialize"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="dematerialize操作符"/>


    <Button
        android:id="@+id/btn_timeinterval"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="timeinterval操作符"/>

    <Button
        android:id="@+id/btn_timestamp"
