        android:layout_height="wrap_content"
        android:text="using操作符"/>

    <Button
        android:id="@+id/btn_tolist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tolist操作符"/>

    <Button
        android:id="@+id/btn_tosortedlist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tosortedlist操作符"/>

    <Button
        android:id="@+id/btn_tomap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tomap操作符"/>

    <Button
        android:id="@+id/btn_tomultimap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="toMultimap操作符"/>


    <Button
        android:id="@+id/btn_nest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="nest操作符"/>
